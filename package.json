  "scripts": {
    "build": "npm run build:umd && npm run build:esm && npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-custom-context-menu.umd.js",
    "build:esm": "rollup --config rollup.config.js --format es --file dist/vue-custom-context-menu.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format umd --file dist/vue-custom-context-menu.min.js --environment MINIFY",
    "watch:umd": "rollup --watch --config rollup.config.js --format umd --file dist/vue-custom-context-menu.umd.js",
